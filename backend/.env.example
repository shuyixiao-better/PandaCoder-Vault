# MySQL 配置 - 用户基础信息数据库
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DATABASE=PandaCoder
MYSQL_USERNAME=root
MYSQL_PASSWORD=你的MySQL密码

# MongoDB 配置 - 文档/周报数据库
# 请根据你的实际情况修改以下配置
MONGODB_HOST=你的MongoDB服务器地址
MONGODB_PORT=27017
MONGODB_DATABASE=PandaCoder
MONGODB_USERNAME=你的MongoDB用户名
MONGODB_PASSWORD=你的MongoDB密码
MONGODB_AUTH_DATABASE=admin

# JWT 配置
# JWT_SECRET 建议使用长随机字符串，用于生成和验证 JWT Token
JWT_SECRET=你的JWT密钥（建议使用长随机字符串）
JWT_EXPIRATION=86400000

# 服务器配置
SERVER_PORT=8080
